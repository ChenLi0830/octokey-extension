{
  "manifest_version": 2,
  "name": "ZenId extension",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [
      "http://localhost:3000/list"
        // "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],

  "background":{
    "scripts":["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png"
  }
}