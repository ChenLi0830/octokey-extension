{
  "manifest_version": 2,
  "name": "ZenId extension",
  "version": "0.1",
  "permissions": [
    "webNavigation",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js",
      "jquery-2.2.0.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost:3000/*",
        "http://zenid.meteor.com/*"
        // "<all_urls>"
      ],
      "js": [
        "content.js",
        "meteor-ddp.js",
        "jquery-2.2.0.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "credentialFill.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}